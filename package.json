  "license": "MIT",
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat compile && tsc",
    "clean": "npx hardhat clean",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
    "deploy:goerli": "npx hardhat run scripts/deploy.ts --network goerli",
    "mint:goerli": "npx hardhat run scripts/mint.ts --network goerli",
    "deploy:mainnet": "npx hardhat run scripts/deploy.ts --network mainnet",
    "mint:mainnet": "npx hardhat run scripts/mint.ts --network mainnet"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "@types/readline-sync": "^1.4.4",
    "chai": "^4.2.0",
    "ethers": "^5.4.7",
    "hardhat": "2.12.0",
    "hardhat-gas-reporter": "^1.0.8",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-rc.1",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@ainft-team/ainft-contracts": "1.0.1",
    "dotenv": "^16.0.3",
    "readline-sync": "^1.4.10"
  }
